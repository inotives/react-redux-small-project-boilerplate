{
  "name": "react-redux-small-project-boilerplate",
  "version": "0.1.0",
  "description": "this is a react-redux boilerplate use for smaller project such as prototyping, simple SPA sites and simple web application",
  "main": "server.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "auto-build": "node_modules/.bin/webpack --watch",
    "prod-build": "node_modules/.bin/webpack -p",
    "dev": "node_modules/.bin/webpack-dev-server --content-base dist",
    "local": "npm run build && node_modules/.bin/http-server -p 1343 dist"
  },
  "keywords": [
    "inotives"
  ],
  "author": "TONI <tonilim85@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "express": "^4.13.4",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.4",
    "react-router": "^2.2.4",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.10.0"
  }
}
